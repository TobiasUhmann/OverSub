{
	"manifest_version": 2,
	
	"name": "OverSub",
	"description": "blend in subtitle overlays for videos",
	"version": "0.4",
	
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	
	"permissions": [
		"activeTab"
	],
	
	"web_accessible_resources" : [
		"content/SubBox/SubBox.*"
	]
}